{
    "name": "fireboostio/php-client",
    "description": "This package is a php client to the fireboostio api.",
    "keywords": [
        "fireboostio",
        "firebostioapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://fireboost.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Bodor Szilard",
            "email": "szilard@fireboost.io"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "FireboostIO\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "FireboostIO\\Test\\" : "test/" }
    }
}
